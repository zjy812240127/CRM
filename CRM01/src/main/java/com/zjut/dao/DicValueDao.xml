<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjut.dao.DicValueDao">


    <select id="getValue" resultType="com.zjut.domain.DicValue">
        select * from tbl_dic_value where typeCode = #{dType} order by orderNo;
