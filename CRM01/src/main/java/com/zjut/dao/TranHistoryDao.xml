<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zjut.dao.TranHistoryDao">
	<insert id="saveTranHistory">
        insert into tbl_tran_history (
            id,
            stage,
            money,
            expectedDate,
            createTime,
            createBy,
            tranId
        )
        values (
           #{id},
           #{stage},
           #{money},
           #{expectedDate},
           #{createTime},
           #{createBy},
           #{tranId}
        )
    </insert>

    <select id="getHistoryById" resultType="com.zjut.domain.TranHistory">
        select * from tbl_tran_history where tranId = #{tranId};
    </select>
  	
</mapper>

































